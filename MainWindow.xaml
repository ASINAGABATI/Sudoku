                <Grid Grid.Row="3" Grid.Column="0" Background="DarkGray" />
                <Grid Grid.Row="3" Grid.Column="2" Background="DarkGray" />
                <Grid Grid.Row="3" Grid.Column="4" Background="DarkGray" />
                <Grid Grid.Row="4" Grid.Column="1" Background="DarkGray" />
                <Grid Grid.Row="4" Grid.Column="3" Background="DarkGray" />
            </Grid>

            <Grid Grid.Column="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="2*" />
                    <RowDefinition Height="5*" />
                    <RowDefinition Height="2*" />
                </Grid.RowDefinitions>
                <Grid Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                    </Grid.RowDefinitions>
                    <Button Grid.Row="0" Content=" " Click="btnNumberItem" />
                    <Button Grid.Row="1" Content="1" Click="btnNumberItem" />
                    <Button Grid.Row="2" Content="2" Click="btnNumberItem" />
                    <Button Grid.Row="3" Content="3" Click="btnNumberItem" />
                    <Button Grid.Row="4" Content="4" Click="btnNumberItem" />
                    <Button Grid.Row="5" Content="5" Click="btnNumberItem" />
                    <Button Grid.Row="6" Content="6" Click="btnNumberItem" />
                    <Button Grid.Row="7" Content="7" Click="btnNumberItem" />
                    <Button Grid.Row="8" Content="8" Click="btnNumberItem" />
                    <Button Grid.Row="9" Content="9" Click="btnNumberItem" />
                </Grid>
            </Grid>
        
            <Grid Grid.Column="3">
                <ListBox Name="lstHistory"  SelectionChanged="lstBoxSelect" SelectionMode="Single" >
                    <ListBox.ContextMenu>
                        <ContextMenu>
                            <MenuItem Header="Delete" Click="deleteGame" />
                        </ContextMenu>
                    </ListBox.ContextMenu>
                </ListBox>

            </Grid>

        </Grid>

        <Grid Grid.Row="1">
            <StatusBar VerticalAlignment="Bottom"  Background="LightGray" >
                <Label Name="sblbl1" Content="" Foreground="Blue" Width="300" />
                <Separator/>
                <Label Name="sblbl2" Content="" />
            </StatusBar>
        </Grid>
        
    </Grid>
</Window>
